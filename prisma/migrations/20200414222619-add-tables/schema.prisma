generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = "***"
}

model Artist {
  id   Int    @default(autoincrement()) @id
  name String @unique
  Song Song[]
}

model Song {
  albumCoverUrl String?
  artistId      Int?
  id            Int     @default(autoincrement()) @id
  name          String
  youtubeId     String?
  Artist        Artist? @relation(fields: [artistId], references: [id])
}